lower=1
upper=100
primenumbers=[]

for num in range(lower, upper+ 1):
   if num > 1:
      for i in range(2, num):
          if (num % i) ==0:
              break
      else:
        primenumbers.append(num)
          
print(primenumbers)
a=[]
b=[]
c=[]
l=[]
j=[]
k=[]
for l in primenumbers len(3):
   
   a.insert(1,l)
print (a)
